/* pages/withDraw/withDraw.wxss */
page{
  background:#f5f5f5;
}
.flex{
  display: flex;
  display: -webkit-flex;

}
.justify-content{
  justify-content: center;
  -webkit-justify-conten:center;
}
.flex-around{
  justify-content: space-around;
  -webkit-justify-content: space-around;
}
.flex-between{
  justify-content: space-between;
  -webkit-justify-content: space-between;
}

.align-items{
  align-items: center;
  -webkit-align-items:center;  
}

.flex-wrap{
  flex-wrap: wrap;
  -webkit-flex-wrap:wrap;
}
.flex-1{
  flex: 1;
  -webkit-flex:1;
}
.flex-end{
  justify-content: flex-end;
  -webkit-justify-content:flex-end;
}
.flex-start{
  justify-content: flex-start;
  -webkit-justify-content:flex-start;
}
.hehe-num-input{
  width: 705rpx;
  margin: 30rpx auto;
  background: white;
  border-radius:10rpx;
}
.qr-code{
  width: 70rpx;
  height: 70rpx;
}
.hehe-total{
  width:650rpx;
  height: 100rpx;
  margin:0 auto;
  font-size:30rpx;
}

.border-bottom{
  border-bottom: 1px solid #f5f5f5;
}

.hehe-input-descrip{
  min-width: 145rpx;
}

.descrip{
  min-width: 70rpx;
}

.redbag-description{
  height: 100rpx;
}
.redbag-description input{
  font-size: 30rpx;
  margin-left: 30rpx;
}
.confirm-redbag{
  width:705rpx;
  height: 96rpx;
  border-radius: 10rpx;
  margin:0 auto;

}
.show-left-fish{
  color:#7d7d7d;
  background: #f5f5f5;
  height: 90rpx;
  width:705rpx;
  margin:0 auto;
  font-size: 30rpx;
  line-height: 90rpx;
}
.input-item{
  padding-right:20rpx;
}
.withDrawSubmit{
  width:100%;
  height: 100%;
  background: #22A9CE;  
  color: white;
  
}